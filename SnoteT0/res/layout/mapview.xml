<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:id="@id/mapview_holder" android:layout_width="fill_parent" android:layout_height="fill_parent" />
    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <LinearLayout android:layout_width="0.0px" android:layout_height="fill_parent" android:layout_weight="1.0" />
        <LinearLayout android:orientation="vertical" android:layout_width="600.0px" android:layout_height="fill_parent" android:layout_weight="0.0">
            <LinearLayout android:layout_width="fill_parent" android:layout_height="0.0px" android:layout_weight="1.0" />
            <LinearLayout android:background="@drawable/snote_page_bg_focus" android:layout_width="fill_parent" android:layout_height="600.0px" android:layout_weight="0.0" />
            <LinearLayout android:layout_width="fill_parent" android:layout_height="0.0px" android:layout_weight="1.0" />
        </LinearLayout>
        <LinearLayout android:layout_width="0.0px" android:layout_height="fill_parent" android:layout_weight="1.0" />
    </LinearLayout>
    <ZoomControls android:gravity="bottom|center" android:id="@id/zoomcontroll" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="30.0px" />
</FrameLayout>
